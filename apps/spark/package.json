{
  "name": "@coremedia-labs/spark",
  "version": "1.0.0",
  "private": true,
  "license": "CoreMedia Open Source License",
  "scripts": {
    "start": "craco start",
    "build": "pnpm set-version && pnpm apollo:codegen && craco build",
    "set-version": "node ./bin/set-version.js",
    "test": "jest",
    "lint": "eslint --fix \"src/**/*.+(ts|tsx)\"",
    "lint:ci": "eslint \"src/**/*.+(ts|tsx)\"",
    "prettier": "prettier --write \"src/**/*.+(ts|tsx)\"",
    "docs": "typedoc src",
    "coverage": "jest --coverage",
    "apollo:prepare": "pnpm download:possible-types && pnpm download:interfaces && pnpm download:metadata && pnpm download:schema && pnpm apollo:codegen",
    "apollo:codegen": "apollo client:codegen --globalTypesFile=./src/__generated__/globalTypes.ts --target typescript",
    "download": "pnpm download:possible-types && pnpm download:interfaces && pnpm download:metadata && pnpm download:schema",
    "download:schema": "node ./bin/download-schema.js",
    "download:possible-types": "node ./bin/download-possible-types.js",
    "download:interfaces": "node ./bin/download-interfaces.js",
    "download:metadata": "node ./bin/download-metadata.js",
    "download:crops": "node ./bin/download-crops.js"
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@coremedia-labs/preview-integration": "workspace:^1.0.0",
    "@coremedia-labs/view-dispatcher": "workspace:^1.0.0",
    "@js-joda/core": "^3.2.0",
    "@js-joda/timezone": "^2.10.0",
    "crypto-hash": "^1.3.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.1.2",
    "react-player": "^2.9.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.2",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-slick": "^0.23.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "apollo": "^2.33.9",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "graphql": "^15.7.2",
    "jest": "26.6.0",
    "prettier": "^2.5.0",
    "sass": "^1.44.0",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.21.9",
    "typescript": "^4.5.3",
    "webpack": "4.44.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
