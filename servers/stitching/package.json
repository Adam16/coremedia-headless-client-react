{
  "name": "@coremedia-labs/headless-server-stitching",
  "version": "1.0.0",
  "private": true,
  "license": "CoreMedia Open Source License",
  "bin": {
    "stitching": "./bin/stitching"
  },
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts\" --exec \"ts-node ./src/index.ts\"",
    "build": "tsc --build && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "test": "exit 0",
    "lint": "eslint --fix \"src/**/*.+(ts|tsx)\"",
    "prettier": "prettier --write \"src/**/*.+(ts|tsx)\"",
    "docs": "exit 0"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@graphql-tools/schema": "^7.0.0",
    "@graphql-tools/stitch": "^7.5.1",
    "@graphql-tools/wrap": "^7.0.1",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-actuator": "^1.7.0",
    "express-graphql": "^0.12.0",
    "express-http-proxy": "^1.6.2",
    "graphql": "^15.5.0",
    "typescript": "^4.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express-actuator": "^1.6.1",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "ts-node": "^10.0.0"
  }
}
