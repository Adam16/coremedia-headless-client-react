{
  "name": "@coremedia-labs/headless-server-stitching",
  "version": "1.0.0",
  "private": true,
  "license": "CoreMedia Open Source License",
  "bin": {
    "stitching": "./bin/stitching"
  },
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts\" --exec \"ts-node ./src/index.ts\"",
    "build": "tsc --build && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "lint": "eslint --fix \"src/**/*.+(ts|tsx)\"",
    "lint:ci": "eslint \"src/**/*.+(ts|tsx)\"",
    "prettier": "prettier --write \"src/**/*.+(ts|tsx)\""
  },
  "dependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@graphql-tools/delegate": "^8.4.2",
    "@graphql-tools/load": "^7.4.1",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/stitch": "^8.4.2",
    "@graphql-tools/url-loader": "^7.5.3",
    "@graphql-tools/utils": "^8.5.3",
    "@graphql-tools/wrap": "^8.3.2",
    "apollo-server": "^3.5.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "cors": "^2.8.5",
    "cross-undici-fetch": "^0.0.26",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-actuator": "^1.8.0",
    "express-graphql": "^0.12.0",
    "express-http-proxy": "^1.6.3",
    "graphql": "^15.7.2",
    "typescript": "^4.5.5",
    "winston": "^3.5.1"
  },
  "devDependencies": {
    "@types/express-actuator": "^1.8.0",
    "@types/node": "^16.11.24",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0"
  }
}
