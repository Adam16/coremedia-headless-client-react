{
  "name": "@coremedia-labs/headless-server-stitching",
  "version": "1.0.0",
  "private": true,
  "license": "CoreMedia Open Source License",
  "bin": {
    "stitching": "./bin/stitching"
  },
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts\" --exec \"ts-node ./src/index.ts\"",
    "build": "tsc --build && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "lint": "eslint --fix \"src/**/*.+(ts|tsx)\"",
    "lint:ci": "eslint \"src/**/*.+(ts|tsx)\"",
    "prettier": "prettier --write \"src/**/*.+(ts|tsx)\""
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@graphql-tools/delegate": "^8.7.7",
    "@graphql-tools/load": "^7.5.10",
    "@graphql-tools/schema": "^8.3.10",
    "@graphql-tools/stitch": "^8.6.9",
    "@graphql-tools/url-loader": "^7.9.20",
    "@graphql-tools/utils": "^8.6.9",
    "@graphql-tools/wrap": "^8.4.16",
    "apollo-server": "^3.7.0",
    "apollo-server-core": "^3.7.0",
    "apollo-server-express": "^3.7.0",
    "cors": "^2.8.5",
    "cross-undici-fetch": "^0.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "express-actuator": "^1.8.1",
    "express-graphql": "^0.12.0",
    "express-http-proxy": "^1.6.3",
    "graphql": "^15.8.0",
    "typescript": "^4.6.4",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express-actuator": "^1.8.0",
    "@types/node": "^16.11.33",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0"
  }
}
